<?xml version="1.0" encoding="ISO-8859-1"?>
<routineuserinput >
    <routinerecord >
        <field name="FromDate" type="date" />
        <field name="ToDate" type="date" />
        <field name="Option" type="integer" />
        <field name="PayMode" type="string" length="20" />
    </routinerecord>
    <routinewindow name="ExportAfipSIFEREWindow" title="Exportaci&#243;n AFIP - SIFERE" >
        <period label="Period" fromfieldname="FromDate" tofieldname="ToDate" />
        <radiobutton label="Type" fieldname="Option" >
            <radiooption label="Withholding" value="0" />
            <radiooption label="Percepcion" value="1" />
        </radiobutton>
        <text label="Payment Mode" fieldname="PayMode" pastewindow="PayModePasteWindow" />
    </routinewindow>
</routineuserinput>
