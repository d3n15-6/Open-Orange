<?xml version="1.0" encoding="ISO-8859-1"?>
<routineuserinput >
    <routinerecord >
        <field name="FromDate" type="date" />
        <field name="ToDate" type="date" />
        <field name="Option" type="integer" defaultvalue="0" />
        <field name="ControlInfo" type="boolean" />
        <field name="Jurisdiction" type="string" length="20" />
    </routinerecord>
    <routinewindow name="ExportARIBWindow" title="Exportaci&#243;n AFIP - ARIB" >
        <period label="Period" fromfieldname="FromDate" tofieldname="ToDate" />
        <radiobutton label="Type" fieldname="Option" >
            <radiooption label="Withholding" value="0" />
            <radiooption label="Percepcion" value="1" />
        </radiobutton>
        <text label="Province" fieldname="Jurisdiction" pastewindow="ProvincePasteWindow" />
        <line>
            <column>
            </column>
            <column>
                <label text="(vacio = BSAS)" />
                <checkbox label="Show Control Info" fieldname="ControlInfo" />
            </column>
        </line>
    </routinewindow>
</routineuserinput>
