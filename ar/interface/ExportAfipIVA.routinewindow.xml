<?xml version="1.0" encoding="ISO-8859-1"?>
<routineuserinput >
    <routinerecord >
        <field name="FromDate" type="date" />
        <field name="ToDate" type="date" />
        <field name="Option" type="integer" />
        <field name="ControlInfo" type="boolean" />
    </routinerecord>
    <routinewindow name="ExportAfipIVAWindow" width="350" title="Exportaci&#243;n AFIP - IVA" >
        <period label="Period" fromfieldname="FromDate" tofieldname="ToDate" />
        <combobox label="Operation Types" fieldname="Option" >
            <combooption label="Withholding" value="0" />
            <combooption label="Percepcion" value="1" />
        </combobox>
        <checkbox label="Show Control Info" fieldname="ControlInfo" />
    </routinewindow>
</routineuserinput>
