    <field name="Height" type="integer" />
</detailrecord>

<detailrecord name="DocumentSpecLabelsRow" tablename="DocumentSpecLabelsRow" >
    <field name="Text" type="string" length="255" />
    <field name="Style" type="string" length="20" linkto="FontStyle" />
    <field name="Alignment" type="integer" />
    <field name="X" type="integer" />
    <field name="Y" type="integer" />
</detailrecord>

<detailrecord name="DocumentSpecRectsRow" tablename="DocumentSpecRectsRow" >
    <field name="X" type="integer" />
    <field name="Y" type="integer" />
    <field name="Width" type="integer" />
    <field name="Height" type="integer" />
</detailrecord>

<record name="EventLog" tablename="EventLog" >
    <field name="TableName" type="string" length="60" />
