    <field name="MailFrom" type="string" length="100" />
    <field name="Subject" type="string" length="60" />
    <field name="TransDate" type="date" />
    <field name="TransTime" type="time" />
    <field name="Status" type="integer" />
    <field name="SentFlag" type="boolean" />
    <field name="ReadFlag" type="boolean" />
    <field name="ProcessedFlag" type="boolean" />
    <field name="MessageBody" type="memo" />
    <field name="RequestReadNotification" type="boolean" />
