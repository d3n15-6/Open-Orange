<?xml version="1.0" encoding="ISO-8859-1"?>
<interface>
<record name="AccessGroup" tablename="AccessGroup" inherits="Master" >
    <field name="Code" type="string" length="5" />
    <field name="Comment" type="string" length="60" />
    <field name="InitialModule" type="string" length="60" />
    <field name="SuperUser" type="boolean" />
    <field name="ModulesAccessType" type="integer" />
    <field name="RecordsAccessType" type="integer" />
    <field name="ReportsAccessType" type="integer" />
    <field name="RoutinesAccessType" type="integer" />
    <field name="CustomsAccessType" type="integer" />
    <field name="Modules" type="detail" recordname="AccessGroupModuleRow" />
    <field name="Records" type="detail" recordname="AccessGroupRecordRow" />
    <field name="Reports" type="detail" recordname="AccessGroupReportRow" />
    <field name="Routines" type="detail" recordname="AccessGroupRoutineRow" />
    <field name="Customs" type="detail" recordname="AccessGroupCustomRow" />
</record>

<detailrecord name="AccessGroupCustomRow" tablename="AccessGroupCustomRow" >
    <field name="Name" type="string" length="60" />
    <field name="Access" type="integer" />
</detailrecord>

<detailrecord name="AccessGroupModuleRow" tablename="AccessGroupModuleRow" >
    <field name="Name" type="string" length="60" />
    <field name="Access" type="integer" />
</detailrecord>

<detailrecord name="AccessGroupRecordRow" tablename="AccessGroupRecordRow" >
    <field name="Name" type="string" length="60" />
    <field name="Access" type="integer" />
    <field name="Visibility" type="integer" />
</detailrecord>

<detailrecord name="AccessGroupReportRow" tablename="AccessGroupReportRow" >
    <field name="Name" type="string" length="60" />
    <field name="Access" type="integer" />
</detailrecord>

<detailrecord name="AccessGroupRoutineRow" tablename="AccessGroupRoutineRow" >
    <field name="Name" type="string" length="60" />
    <field name="Access" type="integer" />
</detailrecord>

<record name="Addressable" tablename="Addressable" inherits="Master" persistent="False" >
    <field name="Address" type="string" length="60" />
    <field name="City" type="string" linkto="City" />
    <field name="Country" type="string" linkto="Country" />
    <field name="Locality" type="string" length="60" />
    <field name="LocalityCode" type="string" linkto="Locality" />
    <field name="Phone" type="string" length="30" />
    <field name="Province" type="string" length="60" />
    <field name="ProvinceCode" type="string" linkto="Province" />
    <field name="ZipCode" type="string" length="20" />
    <field name="Latitude" type="value" />
    <field name="Longitude" type="value" />
</record>

<record name="Attach" tablename="Attach" >
    <field name="SerNr" type="integer" />
    <field name="Type" type="integer" />
    <field name="TransDate" type="date" />
    <field name="TransTime" type="time" />
    <field name="SourceId" type="string" length="255" /> <!-- deprecated -->
    <field name="DestId" type="string" length="255" />
    <field name="OriginRecordName" type="string" length="40" />
    <field name="OriginId" type="string" length="255" />
    <field name="Comment" type="string" length="255" />
    <field name="Value" type="blob" />
    <index name="Origin" >
        <indexfield fieldname="OriginRecordName" />
        <indexfield fieldname="OriginId" />
    </index>
</record>

<record name="BufferSettings" tablename="BufferSettings" setting="true" >
    <field name="Records" type="detail" recordname="BufferSettingsRow" />
</record>

<detailrecord name="BufferSettingsRow" tablename="BufferSettingsRow" >
    <field name="RecordName" type="string" length="200" />
    <field name="Expiration" type="time" />
</detailrecord>

<record name="Classifier" tablename="Classifier" inherits="Master" persistent="false" >
    <field name="Code" type="string" length="20" />
    <field name="Name" type="string" length="60" />
    <field name="Type" type="string" length="20" />
    <field name="Level" type="integer" />
    <field name="PathToRoot" type="string" length="60" />
</record>

<record name="Clauses" tablename="Clauses" inherits="Master" >
    <field name="Code" type="string" length="15" />
    <field name="TextType" type="string" length="20" />
    <field name="Text" type="memo" />
    <field name="Comment" type="string" length="30" />
</record>

<record name="Company" tablename="Company" local="true" >
    <!-- fields order is important-->
    <field name="Code" type="string" length="20" />
    <field name="Name" type="string" length="60" />
    <field name="Host" type="string" length="60" />
    <field name="Port" type="integer" />
    <field name="User" type="string" length="60" />
    <field name="Password" type="string" length="60" />
    <field name="DefaultUser" type="string" length="20" />
    <field name="DefaultUserPassword" type="string" length="60" />
    <field name="ApplicationType" type="integer" />
    <field name="DBName" type="string" length="60" />
    <field name="OfficialSerNr" type="string" length="10" />
    <field name="DBEngine" type="integer" />
</record>

<record name="Computer" tablename="Computer" inherits="Master" >
    <field name="Code" type="string" length="5" />
    <field name="Name" type="string" length="60" />
    <field name="Office" type="string" linkto="Office" />
    <field name="StockDepo" type="string" linkto="StockDepo" />
    <field name="AllowedPayModes" type="set" length="60" linkto="PayMode" />
    <field name="DefaultPayMode" type="string" linkto="PayMode" />
    <field name="OfficialSerNr" type="string" length="20" />
    <field name="DefaultBarTabType" type="string" length="5" />
    <field name="EndOfDay" type="time" />
    <field name="POSflag" type="boolean" />
</record>

<record name="Currency" tablename="Currency" inherits="Master" >
    <field name="Code" type="string" length="3" />
    <field name="Name" type="string" length="30" />
    <field name="BankNotes" type="string" length="80" /> <!-- para guaranies es pesado este campo -->
    <field name="RoundOff" type="integer" />
    <field name="ConvertionBase" type="integer" />
    <field name="ConvertionDirection" type="integer" />
    <field name="FrFactor" type="integer" />
    <field name="ToFactor" type="integer" />
    <field name="RecAcc" type="string" linkto="Account" />    
    <field name="PayAcc" type="string" linkto="Account" />    
    <field name="TaxOfficeCode" type="string" length="5" />
</record>

<record name="DatabaseManagement" tablename="DatabaseManagement" setting="true" >
    <field name="DBPasswordAdminEnabled" type="boolean" />
    <field name="OldDBUser" type="string" length="30" />
    <field name="OldDBPassword" type="string" length="30" />
    <field name="NewDBUser" type="string" length="30" />
    <field name="NewDBPassword" type="string" length="30" />
    <field name="OldDBPasswordExpirationDate" type="date" />
    <field name="PasswordChangesQty" type="integer" />
    <field name="HexBlob" type="boolean" />
    <field name="AddDropDatabase" type="boolean" />
    <field name="AddDropTable" type="boolean" />
    <field name="IgnoreTable" type="string" length="100" />
    <field name="BackupPath" type="string" length="200" />
    <field name="FixMySQLBackup" type="boolean" />
    <field name="ExportOutput" type="memo" persistent="false" />
    <field name="BackupFile" type="string" length="200" />
    <field name="ImportIntoThisDB" type="boolean" />
    <field name="ImpDBName" type="string" length="30" />
    <field name="ImpHost" type="string" length="30" />
    <field name="ImpPort" type="integer" />
    <field name="ImpUser" type="string" length="30" />
    <field name="ImpPassword" type="string" length="30" />
    <field name="MySQLPath" type="string" length="100" />
    <field name="MySQLMaxAllowedPacket" type="integer" persistent="false" />
</record>

<record name="DecimalSpec" tablename="DecimalSpec" inherits="Record" >
    <field name="Currency" type="string" length="3" linkto="Currency" />
    <field name="RecordName" type="string" length="60" />
    <field name="FieldName" type="string" length="20" />
    <field name="RowFieldName" type="string" length="20" />
    <field name="ViewDecimals" type="integer" />
    <field name="RoundDecimals" type="integer" />
</record>

<record name="Department" tablename="Department" inherits="Master" >
    <field name="Code" type="string" length="10" />
    <field name="DeptName" type="string" length="100" />
</record>

<record name="DesktopLayout" tablename="DesktopLayout" local="true" setting="true" >
    <!-- fields order is important-->
    <field name="ToolsBar_Visible" type="boolean" />
    <field name="ToolsBar_Position" type="integer" />
    <field name="ToolsBar_Offset" type="integer" />
    <field name="PersonalBar_Visible" type="boolean" />
    <field name="PersonalBar_Position" type="integer" />
    <field name="PersonalBar_Offset" type="integer" />
    <field name="SearchBar_Visible" type="boolean" />
    <field name="SearchBar_Position" type="integer" />
    <field name="SearchBar_Offset" type="integer" />
    <field name="ModuleBar_Visible" type="boolean" />
    <field name="ModuleBar_Position" type="integer" />
    <field name="ModuleBar_Offset" type="integer" />
</record>

<record name="DetailRecord" tablename="DetailRecord" inherits="RawRecord" persistent="false" >
    <field name="internalId" type="internalId" nullable="false" />
    <field name="masterId" type="masterid" />
    <field name="rowNr" type="integer" />
    <field name="rowId" type="integer" />
    <index name="masterId" >
        <!-- this index can not be unique in order to allow inserting rows in the middle of a matrix-->
        <indexfield fieldname="masterId" />
        <indexfield fieldname="rowNr" />
    </index>
</record>

<record name="DocumentLink" tablename="DocumentLink" inherits="Master" >
    <field name="Code" type="string" length="60" />
    <field name="Specs" type="detail" recordname="DocumentLinkSpecRow" />
</record>

<detailrecord name="DocumentLinkSpecRow" tablename="DocumentLinkSpecRow" >
    <field name="DocumentSpecCode" type="string" length="30" linkto="DocumentSpec" />
    <field name="Office" type="string" length="10" linkto="Office" />
    <field name="Computer" type="string" length="10" />
    <field name="Attribute" type="string" length="20" />
    <field name="ClassName" type="string" length="60" />
    <field name="Conditions" type="string" length="100" />
</detailrecord>

<record name="DocumentPrinter" tablename="DocumentPrinter" inherits="Master" local="true" >
    <field name="Code" type="string" length="20" />
    <field name="PrinterName" type="string" length="60" />
    <field name="OpenPrinterDialog" type="boolean" />
</record>

<record name="DocumentSpec" tablename="DocumentSpec" inherits="Master" >
