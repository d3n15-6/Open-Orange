<record name="Master" tablename="Master" inherits="Record" persistent="false" >
    <field name="Code" type="string" length="20" />
    <field name="Closed" type="Boolean" /> <!--this field is used to filter list and paste windows/-->
    <index name="Code" unique="true" >
        <indexfield fieldname="Code" />
    </index>
    <index name="ClosedCode" >
        <indexfield fieldname="Closed" />
        <indexfield fieldname="Code" />
    </index>
</record>

<record name="Numerable" tablename="Numerable" inherits="Record" persistent="false" >
    <field name="SerNr" type="integer" />
    <field name="ToSerNr" type="integer" /> <!-- used in multipage transactions -->
    <field name="TransDate" type="date" />
    <field name="TransTime" type="time" />
    <field name="Office" type="string" length="10" linkto="Office" />
    <field name="Department" type="string" length="10" linkto="Department" />
    <field name="Computer" type="string" linkto="Computer" />
    <field name="FormType" type="string" linkto="FormTypeDef" />
    <index name="SerNr" unique="true" >
        <indexfield fieldname="SerNr" />
    </index>
    <index name="ToSerNr" unique="true" >
        <indexfield fieldname="ToSerNr" />
    </index>
    <index name="Office" >
        <!--used in SerNr resolution sernrs by Office -->
        <indexfield fieldname="Office" />
        <indexfield fieldname="SerNr" />
    </index>
    <index name="OfficeComputer" >
        <!--used in SerNr resolution sernrs by office by POS -->
        <!-- Added after experience gasstation py: different ranges by office by computer without this field very slow -->
        <indexfield fieldname="Office" />
        <indexfield fieldname="Computer" />
        <indexfield fieldname="SerNr" />
    </index>
    <index name="TransDate" >
        <indexfield fieldname="TransDate" />
        <indexfield fieldname="TransTime" />
    </index>
</record>

<record name="Office" tablename="Office" inherits="Addressable" >
    <field name="Code" type="string" length="10" />
    <field name="Name" type="string" length="60" />
    <field name="ShortCode" type="string" length="5"/>     <!-- Cuando la empresa define codigos largos esto se usa para las exportaciones de impuestos -->
    <field name="CreditPolicy" type="integer" />
    <field name="SalesTarget" type="value" />
    <field name="Salesmen" type="integer" />
    <field name="PayMode" type="string" length="5" linkto="PayMode" />
    <field name="Phone" type="string" length="20" />
    <field name="Fax" type="string" length="20" />
    <field name="Email" type="string" length="60" />
    <field name="PriceDeal" type="string" length="10" linkto="PriceDeal" />
    <field name="DiscountDeal" type="string" length="10" linkto="DiscountDeal" />
    <field name="BranchLabel" type="string" length="60"/>       <!-- Nota that this has No link to !!!  -->
</record>

<record name="OOCase" tablename="OOCase" inherits="Numerable" >
    <field name="Subject" type="string" length="255" />
    <field name="Message" type="memo" persistent="false" />
    <field name="TransDate" type="date" />
    <field name="TransTime" type="time" />
    <field name="OOCaseNr" type="integer" />
    <field name="User" type="string" linkto="User" />
