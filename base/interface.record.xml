    <field name="Field" type="integer" />
    <field name="Message" type="memo" />
    <field name="RecordRow" type="string" length="60" persistent="false" />
    <field name="HTMLRecordViewRows" type="detail" recordname="HTMLRecordViewRow" persistent="false" />
</record>

<detailrecord name="HTMLRecordViewRow" tablename="HTMLRecordViewRow" persistent="False" >
    <field name="Name" type="string" length="30" />
    <field name="Comment" type="string" length="30" />
</detailrecord>

<record name="Label" tablename="Label" inherits="Classifier" >
    <field name="Code" type="string" length="20" />
    <field name="Name" type="string" length="60" />
    <field name="Type" type="string" length="20" linkto="LabelType" />
</record>

<record name="LabelType" tablename="LabelType" inherits="Master" >
    <field name="Name" type="string" length="60" />
</record>

<record name="LocalSettings" tablename="LocalSettings" local="true" setting="True" >
    <field name="Computer" type="string" linkto="Computer" />
