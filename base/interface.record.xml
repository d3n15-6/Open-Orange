<record name="LoginDialog" tablename="LoginDialog" persistent="False" >
    <field name="User" type="string" length="10" />
    <field name="Password" type="string" length="255" />
</record>

<record name="LogSettings" tablename="LogSettings" setting="True" >
    <field name="Enabled" type="boolean" />
    <field name="Records" type="detail" recordname="LogSettingsRecordRow" />
    <field name="Reports" type="detail" recordname="LogSettingsReportRow" />
</record>

<detailrecord name="LogSettingsRecordRow" tablename="LogSettingsRecordRow" >
    <field name="RecordName" type="string" length="50" />
    <field name="Mode" type="integer" />
</detailrecord>

<detailrecord name="LogSettingsReportRow" tablename="LogSettingsReportRow" >
    <field name="ReportName" type="string" length="50" />
    <field name="VersionsQty" type="integer" />
</detailrecord>

<record name="Mail" tablename="Mail" inherits="Numerable" >
