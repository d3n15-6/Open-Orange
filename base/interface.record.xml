<detailrecord name="FormTypeDefRow" tablename="FormTypeDefRow" >
    <field name="Condition" type="string" length="100" />
    <field name="FormType" type="string" length="5" />
</detailrecord>

<record name="HelpDocumentation" tablename="HelpDocumentation" inherits="Master" persistent="False" >
    <field name="Record" type="string" length="100" />
